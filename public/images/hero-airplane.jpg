<svg width="1200" height="600" viewBox="0 0 1200 600" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Sky gradient background -->
  <defs>
    <linearGradient id="heroSky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4facfe;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#00f2fe;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0066cc;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="cloudGrad" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9" />
      <stop offset="100%" style="stop-color:#f0f8ff;stop-opacity:0.6" />
    </radialGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="1200" height="600" fill="url(#heroSky)"/>
  
  <!-- Large clouds -->
  <g opacity="0.7">
    <ellipse cx="200" cy="100" rx="80" ry="40" fill="url(#cloudGrad)"/>
    <ellipse cx="250" cy="90" rx="100" ry="50" fill="url(#cloudGrad)"/>
    <ellipse cx="180" cy="80" rx="60" ry="30" fill="url(#cloudGrad)"/>
  </g>
  
  <g opacity="0.6">
    <ellipse cx="800" cy="150" rx="90" ry="45" fill="url(#cloudGrad)"/>
    <ellipse cx="850" cy="140" rx="110" ry="55" fill="url(#cloudGrad)"/>
    <ellipse cx="780" cy="130" rx="70" ry="35" fill="url(#cloudGrad)"/>
  </g>
  
  <!-- Main airplane -->
  <g transform="translate(600,300)">
    <!-- Airplane body -->
    <ellipse cx="0" cy="0" rx="100" ry="12" fill="#f0f0f0"/>
    <!-- Main wings -->
    <ellipse cx="-20" cy="0" rx="80" ry="6" fill="#e0e0e0"/>
    <!-- Tail wings -->
    <path d="M-80,-3 L-110,-15 L-110,15 L-80,3 Z" fill="#e0e0e0"/>
    <path d="M-85,-12 L-100,-20 L-85,-8 Z" fill="#d0d0d0"/>
    <!-- Windows -->
    <circle cx="40" cy="-3" r="4" fill="#2a5298" opacity="0.8"/>
    <circle cx="25" cy="-3" r="4" fill="#2a5298" opacity="0.8"/>
    <circle cx="10" cy="-3" r="4" fill="#2a5298" opacity="0.8"/>
    <circle cx="-5" cy="-3" r="4" fill="#2a5298" opacity="0.8"/>
    <circle cx="-20" cy="-3" r="4" fill="#2a5298" opacity="0.8"/>
    <!-- Engine highlights -->
    <ellipse cx="-25" cy="-15" rx="8" ry="4" fill="#c0c0c0"/>
    <ellipse cx="-25" cy="15" rx="8" ry="4" fill="#c0c0c0"/>
  </g>
  
  <!-- Distant mountains/horizon -->
  <path d="M0,400 Q200,350 400,380 Q600,360 800,390 Q1000,370 1200,400 L1200,600 L0,600 Z" fill="rgba(255,255,255,0.2)"/>
  
  <!-- Additional smaller clouds for depth -->
  <g opacity="0.4">
    <ellipse cx="100" cy="350" rx="120" ry="60" fill="url(#cloudGrad)"/>
    <ellipse cx="1000" cy="380" rx="140" ry="70" fill="url(#cloudGrad)"/>
  </g>
  
  <!-- Sun rays effect -->
  <g opacity="0.3">
    <path d="M1100,50 L1150,150 L1200,50 Z" fill="#FFD700"/>
    <path d="M1050,80 L1120,180 L1150,80 Z" fill="#FFD700"/>
  </g>
</svg>
